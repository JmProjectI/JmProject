@using JMProject.Model.Sys;
@using JMProject.Common
@using JMProject.Web.Core;
@{
    ViewBag.Title = "订单管理";
    List<permModel> perm = (List<permModel>)ViewBag.Perm;
    if (perm == null)
    {
        perm = new List<permModel>();
    }
}
<script src="@Url.Content("~/Content/My97DatePicker/WdatePicker.js")" type="text/javascript"></script>
<div class="divtool" style="font-size: 12px; height: 60px;">
    <table cellpadding="0" cellspacing="0" border="0">
        <tr style="height: 28px;">
            <td style="min-width: 60px">
                <span>合同编号</span>
            </td>
            <td style="min-width: 200px">
                <input id="txtSearch" class="easyui-textbox" style="width: 190px;" />
            </td>
            <td style="min-width: 60px">
                <span>合同产品</span>
            </td>
            <td style="min-width: 260px">
                <input id="dllOrderItem" name="OrderItem" class="easyui-combotree" style="width: 250px;" />
            </td>
            <td style="min-width: 60px">
                <span>所属地区</span>
            </td>
            <td style="min-width: 190px">
                <input id="DiQuS" class="easyui-combobox" style="width: 180px;" editable="false" />
            </td>
            <td style="min-width: 60px">
                <span>业务员</span>
            </td>
            <td style="min-width: 120px">
                <input id="userS" class="easyui-combobox" style="width: 110px;" editable="false" />
            </td>
            <td style="min-width: 70px">
                <span>回款状态</span>
            </td>
            <td style="min-width: 120px">
                <input id="dzS" class="easyui-combobox" style="width: 110px;" editable="false" />
            </td>
            <td style="min-width: 60px">
                <span>手册状态</span>
            </td>
            <td style="min-width: 130px">
                <select class="easyui-combobox" name="flag" id="ddlflag" style="width: 120px;" panelheight="320">
                    <option value="0">全部</option>
                    <option value="1">未提交</option>
                    <option value="7">已提交</option>
                    <option value="2">A已初审</option>
                    <option value="3">B已派工</option>
                    <option value="5">C已编制完成</option>
                    <option value="4">D用户核对中</option>
                    <option value="6">E已定稿</option>
                    <option value="8">F已装订</option>
                    <option value="9">G装订修改</option>
                    <option value="11">I手册已领取</option>
                    <option value="13">J待定</option>
                    <option value="n4">非用户核对中</option>
                </select>
            </td>
            <td style="min-width: 60px">
                行业
            </td>
            <td style="min-width: 130px">
                <input id="IndustryID" class="easyui-combobox" style="width: 120px;" editable="false" />
            </td>
        </tr>
        <tr style="height: 28px;">
            <td>
                <span>客户名称</span>
            </td>
            <td>
                <input id="NameS" class="easyui-textbox" style="width: 190px;" />
            </td>
            <td>
                <span>合同日期</span>
            </td>
            <td>
