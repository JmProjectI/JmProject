@model JMProject.Model.View.View_Full
@{
    ViewBag.Title = "手册信息";
}
<link href="@Url.Content("~/webuploader-0.1.5/webuploader.css")" rel="stylesheet" type="text/css" />
<link href="@Url.Content("~/Content/smart_wizard_vertical.css")" rel="stylesheet" type="text/css" />
<script src="@Url.Content("~/Scripts/jquery.smartWizard.js")" type="text/javascript"></script>
<script src="@Url.Content("~/webuploader-0.1.5/webuploader.min.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Content/My97DatePicker/WdatePicker.js")" type="text/javascript"></script>
<script src="@Url.Content("~/ScriptsBll/Scxx.js?v=190520")" type="text/javascript"></script>
<script src="@Url.Content("~/ScriptsBll/Scxx_zjsp.js")" type="text/javascript"></script>
<script src="@Url.Content("~/ScriptsBll/Scxx_ksgw.js")" type="text/javascript"></script>
<script src="@Url.Content("~/ScriptsBll/Scxx_fj.js")" type="text/javascript"></script>
<script src="@Url.Content("~/ScriptsBll/Scxx_qlqd.js?v=190508")" type="text/javascript"></script>
<input id="Hiddenid" name="id" type="hidden" value="@Model.NkscModel.id" /><!--手册编号-->
<input id="HiddenFlag" name="Flag" type="hidden" value="@Model.NkscModel.flag" /><!--手册状态-->
<input id="HiddenCustomerID" name="CustomerID" type="hidden" value="@Model.NkscModel.CustomerID" /><!--客户编号-->
<input id="HiddenManager" name="Manager" type="hidden" value="@ViewBag.Manager" /><!--管理员访问-->
<!-- Start 修改金额审批 对话框 -->
<div id="dlgZJ" modal="true" class="easyui-dialog" style="width: 370px; height: 200px;
    padding: 10px;" closed="true" buttons="#dlg-buttons-ZJ">
    金额在
    <input id="txtjineT" name="jineT" class="easyui-numberbox" style="width: 100px;"
        min="0" max="100000000" precision="2" />
    元
    <select class="easyui-combobox" name="jineC" id="dlljineC" editable="false" style="width: 100px;"
        data-options="value:''">
        <option value="以下">以下</option>
        <option value="以上">以上</option>
    </select>
</div>
<div id="dlg-buttons-ZJ">
    <a href="javascript:void(0)" class="easyui-linkbutton" style="width: 60px" onclick="saveZJ()"
        iconcls="icon-save">确定</a> <a href="javascript:void(0)" class="easyui-linkbutton"
            style="width: 60px" onclick="javascript:$('#dlgZJ').dialog('close')" iconcls="icon-cancel">
            取消</a>
</div>
<!-- End 修改金额审批 对话框 -->
<!-- Start 选择 科室 对话框 -->
<div id="dlgKS" modal="true" class="easyui-dialog" style="width: 500px; height: 400px;"
    closed="true" buttons="#dlg-buttons-KS">
    <input id="HiddenKS" name="HiddenKS" type="hidden" value="" />
    <ul id="ulKS" style="list-style-type: none; padding: 5px;">
    </ul>
    <ul id="ulZW" style="list-style-type: none; padding: 5px;">
    </ul>
</div>
<div id="dlg-buttons-KS">
    <a href="javascript:void(0)" class="easyui-linkbutton" onclick="chooseKS()" iconcls="icon-save">
        选择</a> <a href="javascript:void(0)" class="easyui-linkbutton" onclick="closeKS()"
            iconcls="icon-cancel">取消</a>
</div>
<!-- End 选择 科室 对话框 -->
<div id="wizard" class="swMain" style="width: 100%; height: 100%;">
    <ul>
        <li><a href="#step-1"><span class="stepDesc">基本信息</span></a></li>
        <li><a href="#step-2"><span class="stepDesc">领导及职务</span></a></li>
        <li><a href="#step-3"><span class="stepDesc">工作小组</span></a></li>
        <li><a href="#step-4"><span class="stepDesc">负责科室</span></a></li>
        <li><a href="#step-5"><span class="stepDesc">资金审批</span></a></li>
        <li><a href="#step-6"><span class="stepDesc">科室职能</span></a></li>
        <li><a href="#step-7"><span class="stepDesc">相关制度与附件</span></a></li>
        <li><a href="#step-8"><span class="stepDesc">领导权力清单</span></a></li>
        <li><a href="#step-9"><span class="stepDesc">提交&打印&查看</span></a></li>
    </ul>
    <!--基本信息-->
    <div id="step-1">
        <h2 class="StepTitle">
            基本信息 <span style="color: #900; font-size: 14px;">
                @(Model.NkscModel.flag != "1" && Model.NkscModel.flag != "0" && ViewBag.Manager == "0" ? "（信息已提交不能修改，如想修改请联系我们）" : "")
            </span>
        </h2>
        <table class="tab_form" cellpadding="0" cellspacing="0">
            <tr>
                <td class="dec" style="width: 120px;">
                    内控手册颁发日期
                </td>
                <td class="con" style="width: 200px;">
                    <input id="txtdwjc" name="dwjc" type="text" value="@Model.NkscModel.dwjc" onclick="WdatePicker({ minDate: '2015-05-01', maxDate: '%y-%M-%d',dateFmt:'yyyy年 MM月 dd日' })" />
                </td>
                <td class="dec" style="width: 120px;">
                    单位全称
                </td>
                <td class="con">
                    <input id="txtdwqc" name="dwqc" type="text" value="@Model.NkscModel.dwqc" class="easyui-validatebox easyui-tooltip" style="width: 300px"/>
                </td>
            </tr>
            <tr>
                <td class="dec">
                    发票抬头
                </td>
                <td class="con" colspan="3">
                    <input id="txtInvoice" name="dwqc" type="text" value="@ViewBag.Invoice" class="easyui-validatebox easyui-tooltip" style="width: 300px"/>
                    <span style="font-size: 12px; color: Red; font-weight: bold;">注：用于发票，请确认无误</span>
                </td>
            </tr>
            <tr>
                <td class="con" colspan="2">
                    <input id="check_tyxy" type="checkbox" /><label for="check_tyxy" style="color: #2E8B57">本单位发票信息不需要统一社会信用代码</label>
                </td>
                <td class="dec">
                    统一社会信用代码
                </td>
                <td class="con">
                    <input id="TexttyxyID" name="tyxyID" value="@Model.CustomModel.Code" maxlength="20" placeholder="用于开发票" type="text"
                        style="width: 300px" />
                </td>
            </tr>
            <tr>
                <td class="dec">
                    发票收件人
                </td>
                <td class="con">
                    <input id="TextName" name="Name" type="text" placeholder="请填写发票收取人姓名" value="@Model.CustomModel.QtLxr" maxlength="40" />
                </td>
                <td class="dec">
                    手机号
                </td>
                <td class="con">
                    <input id="TextTel" name="Tel" type="text" placeholder="请填写手机号" value="@Model.CustomModel.QtTel" maxlength="50" style="width: 300px" />
                </td>
            </tr>
            <tr>
                <td class="dec">
                    QQ
                </td>
                <td class="con">
                    <input id="TextQQ" name="QQ" type="text" placeholder="请填写QQ号" value="@Model.CustomModel.QQ" maxlength="50" />
                </td>
                <td class="dec">
                    邮寄地址
                </td>
                <td class="con">
                    <input id="TextAddress" name="Address" placeholder="用于邮寄发票，请填写详细邮寄地址" type="text" class="easyui-tooltip"
                        maxlength="500" value="@Model.CustomModel.Address" style="width: 550px" />
                </td>
            </tr>
            <tr>
                <td class="dec">
                    本手册适用范围
                </td>
                <td class="con" colspan="3">
                    <input id="cboxscfw1" name="checkboxscfw" type="checkbox" checked="checked" disabled="disabled"
                        value="本单位各科室" /><label for="cboxscfw1">本单位各科室</label>
                    <input id="cboxscfw2" name="checkboxscfw" type="checkbox" @(Model.NkscModel.syfw0415.IndexOf("事业单位") >= 0 ? "checked='checked'" : "") value="事业单位" /><label
                        for="cboxscfw2">事业单位</label>
                    <input id="cboxscfw3" name="checkboxscfw" type="checkbox" @(Model.NkscModel.syfw0415.IndexOf("派出机构") >= 0 ? "checked='checked'" : "") value="派出机构" /><label
                        for="cboxscfw3">派出机构</label>
                </td>
            </tr>
            <tr>
                <td class="con" colspan="4">
                    <input id="Checkbox_dxkq" type="checkbox" @(string.IsNullOrEmpty(Model.NkscModel.kqsjswSd) ? "" : "checked='checked'") /><label
                        for="Checkbox_dxkq" style="color: #2E8B57">分冬夏 上下班时间</label>
                </td>
            </tr>
            <tr>
                <td class="dec">
                    <span id="spankqsjsw">上午</span>
                </td>
                <td class="con">
                    <input id="txtkqsjswS" name="kqsjswS" type="text" value="@Model.NkscModel.kqsjswS" onclick="WdatePicker({dateFmt:'HH:mm'})" style="width:80px;" />
                    至
                    <input id="txtkqsjswE" name="kqsjswE" type="text" value="@Model.NkscModel.kqsjswE" onclick="WdatePicker({dateFmt:'HH:mm'})" style="width:80px;" />
                </td>
                <td class="dec">
                    <span id="spankqsjxw">下午</span>
                </td>
                <td class="con">
                    <input id="txtkqsjxwS" name="kqsjxwS" type="text" value="@Model.NkscModel.kqsjxwS" onclick="WdatePicker({dateFmt:'HH:mm'})" style="width:80px;" />
                    至
                    <input id="txtkqsjxwE" name="kqsjxwE" type="text" value="@Model.NkscModel.kqsjxwE" onclick="WdatePicker({dateFmt:'HH:mm'})" style="width:80px;" />
                </td>
            </tr>
            <tr id="dxkqsj" style="display: none;">
                <td class="dec">
                    冬令时：上午
                </td>
                <td class="con">
                    <input id="txtkqsjswSd" name="kqsjswSd" type="text" value="@Model.NkscModel.kqsjswSd" onclick="WdatePicker({dateFmt:'HH:mm'})" style="font-size:12px;width:80px;" />
                    至
                    <input id="txtkqsjswEd" name="kqsjswEd" type="text" value="@Model.NkscModel.kqsjswEd" onclick="WdatePicker({dateFmt:'HH:mm'})" style="font-size:12px;width:80px;" />
                </td>
                <td class="dec">
                    冬令时：下午
                </td>
                <td class="con">
                    <input id="txtkqsjxwSd" name="kqsjxwSd" type="text" value="@Model.NkscModel.kqsjxwSd" onclick="WdatePicker({dateFmt:'HH:mm'})" style="font-size:12px;width:80px;" />
                    至
                    <input id="txtkqsjxwEd" name="kqsjxwEd" type="text" value="@Model.NkscModel.kqsjxwEd" onclick="WdatePicker({dateFmt:'HH:mm'})" style="font-size:12px;width:80px;" />
                </td>
            </tr>
            <tr>
                <td class="dec">
                    本单位的所有科室
                </td>
                <td class="con" colspan="3">
                    <input id="txtbhks" name="bhks" type="text" value="@Model.NkscModel.bhks" placeholder="每项之间使用、号分隔" class="easyui-textbox"
                        style="width: 1050px;" />
                </td>
            </tr>
            <tr style="height: 262px">
                <td class="dec">
                    单位简介
                </td>
                <td class="con" colspan="3">
                    <textarea id="txtdwjj" name="dwjj" rows="7" maxlength="4000" class="easyui-validatebox"
                        placeholder="一定要包含历史沿革、取得成绩、机构情况、人员编制，主要职责，也可将单位公共受托目标和责任、单位编制情况（如行政、事业、工勤、离退休编制和实有编制情况）等内容写上"
                        style="height: 250px; width: 1050px; font-size: 12px;">@Model.NkscModel.dwjj</textarea>
                </td>
            </tr>
        </table>
    </div>
    <!--领导及职务-->
    <div id="step-2">
        <h2 class="StepTitle">
            领导及职务<span style="color: #900; font-size: 14px;">
                @(Model.NkscModel.flag != "1" && Model.NkscModel.flag != "0" && ViewBag.Manager == "0" ? "（信息已提交不能修改，如想修改请联系我们）" : "")
            </span>
        </h2>
        <table class="tab_form" cellpadding="0" cellspacing="0">
            <tr>
                <td class="dec">
                    党组织机构名称
                </td>
                <td class="con" colspan="5">
                    <input id="Radio_dzzjgmc2" type="radio" name="dzzjgmc" value="党委" @(Model.NkscModel.dzzjgmc == "党委" ? "checked='checked'" : "") /><label
                        for="Radio_dzzjgmc2">党委</label>
                    <input id="Radio_dzzjgmc3" type="radio" name="dzzjgmc" value="党支部" @(Model.NkscModel.dzzjgmc == "党支部" ? "checked='checked'" : "") /><label
                        for="Radio_dzzjgmc3">党支部</label>
                    <input id="Radio_dzzjgmc1" type="radio" name="dzzjgmc" value="党组" @(Model.NkscModel.dzzjgmc == "党组" ? "checked='checked'" : "") /><label
                        for="Radio_dzzjgmc1">党组</label>
                    <input id="Radio_dzzjgmc5" type="radio" name="dzzjgmc" value="党工委" @(Model.NkscModel.dzzjgmc == "党工委" ? "checked='checked'" : "") /><label
                        for="Radio_dzzjgmc5">党工委</label>
                    <input id="Radio_dzzjgmc6" type="radio" name="dzzjgmc" value="党总支" @(Model.NkscModel.dzzjgmc == "党总支" ? "checked='checked'" : "") /><label
                        for="Radio_dzzjgmc6">党总支</label>
                    <input id="Radio_dzzjgmc4" type="radio" name="dzzjgmc" value="无" @(Model.NkscModel.dzzjgmc == "无" ? "checked='checked'" : "") /><label
                        for="Radio_dzzjgmc4">无</label>
                </td>
            </tr>
            <tr>
                <td class="dec">
                    正职职务名称
                </td>
                <td class="con">
                    <input id="txtzzzwmc" name="zzzwmc" type="text" value="@Model.NkscModel.zzzwmc" placeholder="如局长、主任" class="easyui-textbox" />
                </td>
                <td class="dec">
                    正职领导姓名
                </td>
                <td class="con">
                    <input id="txtldzzmc" name="ldzzmc" type="text" value="@Model.NkscModel.ldzzmc" class="easyui-textbox" />
                </td>
                <td class="dec">
                    <span id="spanzzzwDY">@(string.IsNullOrEmpty(Model.NkscModel.dzzjgmc) ? "" : string.Format("是否{0}成员", Model.NkscModel.dzzjgmc))</span>
                </td>
                <td class="con" style="min-width: 208px;">
                    <div id="divzzzwDY" style="@(string.IsNullOrEmpty(Model.NkscModel.dzzjgmc) ? "display:none;" : "")">
                        <input id="Radio_zzzwDY1" type="radio" name="zzzwDY" value="1" @(Model.NkscModel.zzzwDY == "1" ? "checked='checked'" : "") /><label
                            for="Radio_zzzwDY1">是</label>
                        <input id="Radio_zzzwDY2" type="radio" name="zzzwDY" value="0" @(Model.NkscModel.zzzwDY != "1" ? "checked='checked'" : "") /><label
                            for="Radio_zzzwDY2">否</label>
                    </div>
                </td>
            </tr>
            <tr>
                <td class="dec">
                    正职领导分管科室
                </td>
                <td class="con" colspan="5">
                    <input jm="com" id="txtldzzfg" name="ldzzfg" type="text" value="@Model.NkscModel.ldzzfg" placeholder="如没有分管科室请不用填写文字。如有就直接填写科室名称 [科室名称之间用顿号(、)分隔]" class="easyui-textbox" style="width:1050px" />
                </td>
            </tr>
            <tr>
                <td class="dec">
                    副职职务名称1
                </td>
                <td class="con">
                    <input id="txtfzzwmc1" name="fzzwmc1" type="text" value="@Model.NkscModel.fzzwmc1" placeholder="如副局长、副主任" class="easyui-textbox" />
                </td>
                <td class="dec">
                    副职领导姓名1
                </td>
                <td class="con">
                    <input id="txtldfzmc1" name="ldfzmc1" type="text" value="@Model.NkscModel.ldfzmc1" class="easyui-textbox" />
                </td>
                <td class="dec">
                    <span id="spanfzzwDY">@(string.IsNullOrEmpty(Model.NkscModel.dzzjgmc) ? "" : string.Format("是否{0}成员", Model.NkscModel.dzzjgmc))</span>
                </td>
                <td class="con">
                    <div id="divfzzwDY" style="@(string.IsNullOrEmpty(Model.NkscModel.dzzjgmc) ? "display:none;" : "")">
                        <input id="Radio_fzzwDY1" type="radio" name="fzzwDY" value="1" @(Model.NkscModel.fzzwDY == "1" ? "checked='checked'" : "") /><label
                            for="Radio_fzzwDY1">是</label>
                        <input id="Radio_fzzwDY2" type="radio" name="fzzwDY" value="0" @(Model.NkscModel.fzzwDY != "1" ? "checked='checked'" : "") /><label
                            for="Radio_fzzwDY2">否</label>
                    </div>
                </td>
            </tr>
            <tr>
                <td class="dec">
                    副职领导1分管科室
                </td>
                <td class="con" colspan="5">
                    <input jm="com" id="txtldfzfg1" name="ldfzfg1" type="text" value="@Model.NkscModel.ldfzfg1" placeholder="填写如[财务科、综合科...](用、号分隔)" class="easyui-textbox" style="width:1050px" />
                </td>
            </tr>
            <tr>
                <td class="con" colspan="6">
                    <a href="javascript:void(0)" class="easyui-linkbutton" iconcls="icon-add" onclick="addfz()">
                        添加副职</a> <a href="javascript:void(0)" class="easyui-linkbutton" iconcls="icon-cancel"
                            onclick="removefz()" style="margin-left: 10px;">删除副职</a> <span style="color: #900;">
                                （更多的副职领导请点击左侧“添加副职”按钮）</span>
                </td>
            </tr>
        </table>
        <table id="table_fz" class="tab_form" cellpadding="0" cellspacing="0">
            @foreach (var item in Model.Nksc_fzModel)
            {
                <tr>
                    <td class="dec">
                        <input id="@string.Format("check_fz{0}", item.sort)" type="checkbox" name="check_fz" /><label for="@string.Format("check_fz{0}", item.sort)">@string.Format("副职职务名称{0}", item.sort)</label>
                    </td>
                    <td class="con">
                        <input id="@string.Format("txtfzzwmc{0}", item.sort)" name="@string.Format("fzzwmc{0}", item.sort)" type="text" value="@item.fzzwmc" class="easyui-textbox" />
                    </td>
                    <td class="dec">
                        @string.Format("副职领导姓名{0}", item.sort)
                    </td>
                    <td class="con">
                        <input id="@string.Format("txtldfzmc{0}", item.sort)" name="@string.Format("ldfzmc{0}", item.sort)" type="text" value="@item.ldfzmc" class="easyui-textbox" />
                    </td>
                    <td class="dec">
                        <span id="@string.Format("spanfzzwDY{0}", item.sort)">@(string.IsNullOrEmpty(Model.NkscModel.dzzjgmc) ? "" : string.Format("是否{0}成员", Model.NkscModel.dzzjgmc))</span>
                    </td>
                    <td class="con" style="min-width: 208px;">
                        <div id="@string.Format("divfzzwDY{0}", item.sort)" style="@(string.IsNullOrEmpty(Model.NkscModel.dzzjgmc) ? "display:none;" : "")">
                            <input id="@string.Format("Radio_fzzwDY1{0}", item.sort)" type="radio" name="@string.Format("fzzwDY{0}", item.sort)" value="1" @(item.fzzwDY == "1" ? "checked='checked'" : "") /><label for="@string.Format("Radio_fzzwDY1{0}", item.sort)">是</label>
                            <input id="@string.Format("Radio_fzzwDY2{0}", item.sort)" type="radio" name="@string.Format("fzzwDY{0}", item.sort)" value="0" @(item.fzzwDY != "1" ? "checked='checked'" : "") /><label for="@string.Format("Radio_fzzwDY2{0}", item.sort)">否</label>
                        </div>
                    </td>
                </tr> 
                <tr>
                    <td class="dec">
                        @string.Format("副职领导{0}分管科室", item.sort)
                    </td>
                    <td class="con" colspan="5">
                        <input id="@string.Format("txtldfzfg{0}", item.sort)" name="@string.Format("ldfzfg{0}", item.sort)" type="text" value="@item.ldfzfg" class="easyui-textbox" style="width: 1000px" />
                    </td>
                </tr>
            }
        </table>
    </div>
    <!--工作小组-->
    <div id="step-3">
        <h2 class="StepTitle">
            工作小组<span style="color: #900; font-size: 14px;">
                @(Model.NkscModel.flag != "1" && Model.NkscModel.flag != "0" && ViewBag.Manager == "0" ? "（信息已提交不能修改，如想修改请联系我们）" : "")
            </span>
        </h2>
        <table class="tab_form" cellpadding="0" cellspacing="0">
            <tr>
                <td class="dec">
                    手册编制牵头部门
                </td>
                <td class="con">
                    <input jm="com" id="txtscqtbm" name="scqtbm" type="text" value="@Model.NkscModel.scqtbm" placeholder="如财务科或办公室等，不能与风险评估小组和监督检查小组同一科室。" class="easyui-textbox" />
                </td>
                <td class="dec">
                    手册编制协作部门
                </td>
                <td class="con">
                    <input jm="com" id="txtscxzbm" name="scxzbm" type="text" value="@Model.NkscModel.scxzbm" placeholder="一般是各科室内控联络员为成员，如办公室张三、人教科李四（用、号分隔）【推荐填写科室名称+科室内控联络员姓名】" class="easyui-textbox" style="width:600px" />
                </td>
            </tr>
            <tr>
                <td class="con" colspan="4" style="color: #2E8B57">
                    内部控制领导小组
                </td>
            </tr>
            <tr>
                <td class="dec">
                    内部控制领导小组：成员信息
                </td>
                <td class="con" colspan="3">
