@{
    ViewBag.Title = "文件上传";
}
<link href="@Url.Content("~/webuploader-0.1.5/webuploader.css")" rel="stylesheet" type="text/css" />
<script src="@Url.Content("~/webuploader-0.1.5/webuploader.min.js")" type="text/javascript"></script>
<script src="@Url.Content("~/ScriptsBll/UploadFile.js")" type="text/javascript"></script>
<style>
    .tableM{font-size: 14px;}
    .tableM tr{height: 35px;}
    .tableM td{padding-left: 5px;}
    .errMsg{color: Red;}
    .sucMsg{color: Green;}
    .webuploader-pick
    {
        background: #00b7ee;
        padding: 5px 10px;
        border-radius: 3px;
    }
    .webuploader-pick-hover
    {
        background: #00a2d4;
    }
</style>
<table cellpadding="0" cellspacing="0" border="0" class="tableM">
    <tr style="height: 10px;">
        <td style="min-width: 50px;">
        </td>
        <td style="min-width: 100px;">
        </td>
        <td style="min-width: 180px;">
        </td>
        <td style="min-width: 50px;">
        </td>
    </tr>
    <tr>
        <td>
        </td>
        <td>
            文件名
        </td>
        <td>
            <span id="spanfile" style="font-size: 13px;"></span>
        </td>
        <td>
        </td>
    </tr>
    <tr>
        <td>
        </td>
        <td>
            SheetName
        </td>
        <td>
            <input name="SheetName" class="easyui-combobox" id="ddlSheetName" editable="false"
                style="width: 200px;" />
        </td>
        <td>
        </td>
    </tr>
    <tr>
        <td>
        </td>
        <td>
            <div id="uploader" class="wu-example">
                <!--用来存放文件信息-->
                <div id="thelist" class="uploader-list">
                </div>
                <div class="btns">
                    <div id="picker">
                        选择文件</div>
                </div>
            </div>
        </td>
        <td>
            <input id="btnUpload" class="ExportButton" value="导入数据" type="button" />
        </td>
        <td>
        </td>
    </tr>
</table>
<span class="sucMsg"></span>
<span class="errMsg" style="white-space: pre-wrap;">
</span>